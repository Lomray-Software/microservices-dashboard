{
  "name": "@lomray/microservices-dashboard",
  "version": "1.0.0",
  "description": "Microservices Dashboard",
  "keywords": [
    "js",
    "microservices",
    "microservice",
    "inverted",
    "json",
    "dashboard",
    "admin",
    "typescript",
    "react",
    "afterjs",
    "razzle"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Lomray-Software/microservices-dashboard.git"
  },
  "bugs": {
    "url": "https://github.com/Lomray-Software/microservices-dashboard/issues"
